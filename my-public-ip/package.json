{
  "name": "my-public-ip",
  "displayName": "My Public Ip",
  "description": "Display your public ip on the status bar",
  "version": "0.0.8",
  "publisher": "emanuelebartolesi",
  "author": {
    "name": "Emanuele Bartolesi"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/kasuken/vscodeextensions/issues",
    "email": "bartolesiemanuele@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kasuken/vscodeextensions.git"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#FFFFFF",
    "theme": "light"
  },
  "homepage": "https://github.com/kasuken/vscodeextensions/blob/master/README.md",
  "engines": {
    "vscode": "^1.30.0"
